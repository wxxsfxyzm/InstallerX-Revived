    <string name="instant_app_desc">Install the package as an instant app.</string>
    <string name="dont_kill_app">Don\'t Kill App</string>
    <string name="dont_kill_app_desc">Used when installing a feature for a split app to prevent the main app from being killed during install.</string>
    <string name="full_app">Full App</string>
    <string name="full_app_desc">Indicates that the package being installed is a full app. Can be used to upgrade instant apps to full apps.</string>
    <string name="allocate_aggressive">Allocate Aggressive</string>
    <string name="allocate_aggressive_desc">Indicates that the package is \"critical to system health or security\" and will more aggressively delete clearable files to make space if needed.</string>
    <string name="virtual_preload">Virtual Preload</string>
    <string name="virtual_preload_desc">Indicates that the package is a virtual preload.</string>
    <string name="apex" translatable="false">APEX</string>
    <string name="apex_desc">Indicates that the package being installed is an APEX package</string>
    <string name="enable_rollback">Enable Rollback</string>
    <string name="enable_rollback_desc">Allow future rollbacks of this package.</string>
    <string name="disable_verification">Disable Verification</string>
    <string name="disable_verification_desc">Disables package verification during install. This is not signature verification.</string>
    <string name="staged">Staged</string>
    <string name="staged_desc">Indicates this package is being installed as part of a staged install.</string>
    <string name="config_all_whitelist_restricted_permissions">Allow Restricted Permissions</string>
    <string name="config_all_whitelist_restricted_permissions_desc">Allow restricted permissions to be granted for the package being installed.</string>
    <string name="disable_allowed_apex_update_check">Disable Allowed APEX Update Check</string>
    <string name="disable_allowed_apex_update_check_desc">Don\'t check whether the installed APEX can be updated.</string>
    <string name="config_bypass_low_target_sdk">Bypass Low Target SDK Block</string>
    <string name="config_bypass_low_target_sdk_desc">Allows apps targeting old SDK versions to be installed.</string>
    <string name="dry_run">Dry Run</string>
    <string name="dry_run_desc">Verify the package, but don\'t install it.</string>

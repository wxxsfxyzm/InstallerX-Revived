    <string name="config_grant_all_permissions">Grant All Requested Permissions</string>
    <string name="config_grant_all_permissions_desc">Automatically grant all permissions the package is requesting. Note that this only applies to runtime permissions. Special permissions like All Files Access or Accessibility won\'t be automatically granted.</string>
    <string name="force_volume_uuid">Force Volume UUID</string>
    <string name="force_volume_uuid_desc">Allows specifying a volume UUID to install the package to.</string>
    <string name="force_permission_prompt">Force Permission Prompt</string>
    <string name="force_permission_prompt_desc">Forces even system installers to show the install confirmation prompt.</string>
